<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Test" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#FF4B3A</item>
        <item name="colorPrimaryVariant">#FF4B3A</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#FF4B3A</item>
        <item name="colorSecondaryVariant">#FF4B3A</item>
        <item name="colorOnSecondary">@color/black</item>
        <item name="android:colorAccent">#FF4B3A</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="circle">
        <item name="cornerSize">50%</item>
    </style>

    <style name="rounded">
        <item name="cornerSize">10%</item>
    </style>

</resources>